# 数据库配置
DB_NAME=storage_db
DB_USER=root
DB_PASSWORD=your_password
DB_HOST=localhost
DB_PORT=3306

# Django密钥（生产环境请使用强随机值）
SECRET_KEY=your_secret_key_here_change_in_production

# 调试模式（生产环境必须设置为False）
DEBUG=True

# 允许的主机
ALLOWED_HOSTS=127.0.0.1,localhost

# Redis配置（如果使用Redis作为缓存和任务队列）
REDIS_URL=redis://localhost:6379/0

# 邮件配置
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=your_email@example.com
EMAIL_HOST_PASSWORD=your_email_password
DEFAULT_FROM_EMAIL=your_email@example.com

# 第三方登录配置
# 微信登录
WECHAT_APP_ID=your-wechat-app-id
WECHAT_APP_SECRET=your-wechat-app-secret
WECHAT_REDIRECT_URI=http://your-domain.com/users/oauth/wechat/callback/

# QQ登录
QQ_APP_ID=your-qq-app-id
QQ_APP_KEY=your-qq-app-key
QQ_REDIRECT_URI=http://your-domain.com/users/oauth/qq/callback/

# 支付宝登录
ALIPAY_APP_ID=your-alipay-app-id
ALIPAY_PRIVATE_KEY=your-alipay-private-key
ALIPAY_PUBLIC_KEY=your-alipay-public-key
ALIPAY_REDIRECT_URI=http://your-domain.com/users/oauth/alipay/callback/
ALIPAY_SANDBOX=True  # True表示使用沙箱环境，False表示使用生产环境

# 文件上传配置
MEDIA_ROOT=media/
MEDIA_URL=/media/

# 静态文件配置
STATIC_ROOT=staticfiles/
STATIC_URL=/static/

# 安全配置
CSRF_COOKIE_SECURE=False  # 生产环境设置为True
SESSION_COOKIE_SECURE=False  # 生产环境设置为True
SECURE_SSL_REDIRECT=False  # 生产环境设置为True

# 日志级别
LOG_LEVEL=INFO

# 分页配置
DEFAULT_PAGINATION=10

# 标签打印配置
DEFAULT_LABEL_WIDTH=60
DEFAULT_LABEL_HEIGHT=40
DEFAULT_FONT_SIZE=12

# Supabase配置
SUPABASE_URL=https://your-project-url.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here
SUPABASE_DB_URL=postgresql://postgres:postgres@db.supabase.co:5432/postgres
